<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.fi.activity"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ServiceActivity"/>
        <activity android:name=".ServiceActivity2"/>
        <activity android:name=".ServiceActivity3"/>

        <service android:name="com.fi.service.TestService"/>
        <service android:name="com.fi.service.TestService2"/>
        <service android:name="com.fi.service.TestService3"/>
        <service android:name="com.fi.service.MyIntentService"/>

        <activity android:name=".DetailActivity">
        </activity>
        <activity android:name=".IntentServiceActivity">
        </activity>
    </application>

</manifest>
